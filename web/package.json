{
  "name": "dash-frontend",
  "version": "0.0.0",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "node --max-old-space-size=8000 ./node_modules/@angular/cli/bin/ng build --prod --output-hashing=all",
    "test": "ng test --browsers=ChromeDebug",
    "test:headless": "ng test --watch=false --browsers=ChromeHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prettier": "prettier --write \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss}\"",
    "electron:serve": "wait-on http-get://localhost:7777 && npm run electron:serve-tsc && electron . --serve",
    "electron:serve-tsc": "tsc -p tsconfig-serve.json"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "arrowParens": "avoid",
    "singleQuote": true
  },
  "build": {
    "appId": "com.vmware.tanzu.octant"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.0",
    "@angular/common": "^9.1.0",
    "@angular/compiler": "^9.1.0",
    "@angular/core": "^9.1.0",
    "@angular/forms": "^9.1.0",
    "@angular/platform-browser": "^9.1.0",
    "@angular/platform-browser-dynamic": "^9.1.0",
    "@angular/router": "^9.1.0",
    "@clr/angular": "^3.0.1",
    "@clr/core": "^3.0.1",
    "@clr/icons": "^3.0.1",
    "@clr/ui": "^3.0.1",
    "@ng-select/ng-select": "^3.7.3",
    "@types/cytoscape": "^3.14.0",
    "@webcomponents/custom-elements": "^1.4.1",
    "angular-resizable-element": "^3.3.0",
    "ansi_up": "^4.0.4",
    "core-js": "^3.6.4",
    "cytoscape": "^3.14.1",
    "cytoscape-dagre": "^2.2.2",
    "cytoscape-node-html-label": "^1.2.0",
    "d3-graphviz": "^3.0.4",
    "d3-selection": "^1.4.1",
    "d3-zoom": "^1.8.3",
    "dagre-d3": "^0.6.4",
    "decimal.js": "^10.2.0",
    "default-passive-events": "^1.0.10",
    "highlight.js": "^9.18.1",
    "lodash": "^4.17.15",
    "lodash-es": "^4.17.15",
    "monaco-editor": "^0.20.0",
    "ng-monaco-editor": "^2.2.2",
    "ngx-highlightjs": "^3.0.3",
    "ngx-markdown": "^8.2.1",
    "rxjs": "^6.5.4",
    "serialize-javascript": "^3.0.0",
    "tslib": "^1.11.1",
    "uuid": "^7.0.2",
    "ws": "^7.2.3",
    "xterm": "^4.4.0",
    "xterm-addon-fit": "^0.3.0",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.901.0",
    "@angular/cli": "^9.1.0",
    "@angular/compiler-cli": "^9.1.0",
    "@angular/language-service": "^9.1.0",
    "@types/d3": "~5.7.2",
    "@types/graphlib-dot": "^0.6.1",
    "@types/jasmine": "~3.5.10",
    "@types/jasminewd2": "^2.0.8",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.12.31",
    "@types/ws": "^7.2.3",
    "codelyzer": "^5.2.2",
    "dayjs": "^1.8.23",
    "electron": "^8.2.0",
    "electron-builder": "^22.4.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "^3.1.1",
    "karma-jasmine-html-reporter": "^1.5.3",
    "karma-spec-reporter": "0.0.32",
    "prettier": "^2.0.2",
    "prettier-tslint": "^0.4.2",
    "protractor": "^5.4.3",
    "ts-node": "8.8.1",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^3.8.3",
    "wait-on": "^4.0.1"
  }
}
