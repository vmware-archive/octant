    "@types/d3": "~5.16.3",
    "@types/graphlib-dot": "^0.6.1",
    "@types/jasmine": "^3.5.14",
    "@types/jasminewd2": "^2.0.8",
    "@types/lodash": "^4.14.161",
    "@types/node": "^14.11.8",
    "@types/ws": "^7.2.7",
    "babel-loader": "^8.1.0",
    "codelyzer": "^6.0.1",
