  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1102.1",
    "@angular/cli": "^11.2.7",
    "@angular/compiler-cli": "^11.2.8",
    "@angular/language-service": "^11.2.8",
    "@babel/core": "^7.11.6",
    "@compodoc/compodoc": "^1.1.11",
    "@mdx-js/loader": "^1.6.18",
    "@storybook/addon-actions": "^v6.1.20",
    "@storybook/addon-console": "^1.2.2",
    "@storybook/addon-docs": "^v6.1.20",
    "@storybook/addon-knobs": "^v6.1.20",
