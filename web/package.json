{
  "name": "octant",
  "description": "Octant is an extensible platform that provides modules that help developers better understand their Kuberenetes resources.",
  "author": "The Octant Team",
  "version": "0.19.0",
  "homepage": "https://octant.dev",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vmware-tanzu/octant.git"
  },
  "bugs": {
    "url": "https://github.com/vmware-tanzu/octant/issues"
  },
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "node --max-old-space-size=8000 ./node_modules/@angular/cli/bin/ng build --prod --output-hashing=all",
    "build-electron": "npm install && tsc main.ts && npm run build -- --base-href ./",
    "test": "ng test --watch=false --browsers=ElectronDebug",
    "test:headless": "ng test --watch=false --browsers=ChromeHeadless --source-map=false",
    "lint": "prettier -c 'src/**/*.ts'",
    "clean": "rimraf ./dist/octant/*",
    "e2e": "ng e2e",
    "prettier": "prettier --write \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss}\"",
    "electron:serve": "wait-on http-get://localhost:7777 && npm run electron:serve-tsc && electron . --serve",
    "electron:serve-tsc": "npx tsc -p tsconfig.serve.json",
    "electron:local": "npx tsc main.ts && npx ng build --base-href ./ && npx electron --local .",
    "electron:build": "npm run build-electron && npx electron-builder -p never",
    "//": [
      "switch to tsconfig.json below once compodoc angular 9 bug is fixed (https://github.com/compodoc/compodoc/issues/951)",
      "see also https://github.com/compodoc/compodoc/issues/927#issuecomment-597624654"
    ],
    "docs:json": "compodoc -p tsconfig.compodoc.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 6006 -s src/assets",
    "build-storybook": "npm run docs:json && node --max-old-space-size=8000 ./node_modules/@storybook/angular/bin/build.js build-storybook -c .storybook",
    "preinstall": "npx npm-force-resolutions",
    "postinstall": "rm -rf ./node_modules/cytoscape-node-html-label/dist/cytoscape-node-html-label.d.ts"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "arrowParens": "avoid",
    "singleQuote": true
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.2.8",
    "@angular/common": "^11.2.8",
    "@angular/compiler": "^11.2.8",
    "@angular/core": "^11.2.8",
    "@angular/forms": "^11.2.8",
    "@angular/platform-browser": "^11.2.8",
    "@angular/platform-browser-dynamic": "^11.2.8",
    "@angular/router": "^11.2.8",
    "@cds/core": "^5.1.0",
    "@clr/angular": "^5.1.0",
    "@clr/icons": "^5.1.0",
    "@clr/ui": "^5.1.0",
    "@materia-ui/ngx-monaco-editor": "^5.0.0",
    "@ng-select/ng-select": "^5.0.1",
    "@types/cytoscape": "^3.14.12",
    "@types/overlayscrollbars": "^1.12.0",
    "@webcomponents/custom-elements": "^1.4.2",
    "angular-resizable-element": "^3.3.3",
    "ansi_up": "^5.0.0",
    "core-js": "^3.6.5",
    "cytoscape": "^3.18.1",
    "cytoscape-cose-bilkent": "^4.1.0",
    "cytoscape-dagre": "^2.3.0",
    "cytoscape-node-html-label": "^1.2.2",
    "d3-graphviz": "^2.6.1",
    "d3-zoom": "^2.0.0",
    "dagre-d3": "^0.6.4",
    "electron-store": "^5.2.0",
    "get-port": "^5.1.1",
    "highlight.js": "^10.2.1",
    "karma-electron": "^6.3.3",
    "karma-electron-launcher": "^0.3.0",
    "lodash": "^4.17.20",
    "lodash-es": "^4.17.15",
    "monaco-editor": "^0.21.2",
    "ngx-highlightjs": "^4.1.2",
    "overlayscrollbars-ngx": "^0.2.2",
    "rxjs": "^6.6.3",
    "serialize-javascript": "^5.0.1",
    "tslib": "^2.0.3",
