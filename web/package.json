{
  "name": "dash-frontend",
  "version": "0.0.0",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "node --max-old-space-size=8000 ./node_modules/@angular/cli/bin/ng build --prod --output-hashing=all",
    "test": "ng test --browsers=ChromeDebug",
    "test:headless": "ng test --watch=false --browsers=ChromeHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prettier": "prettier --write \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss}\"",
    "electron:serve": "wait-on http-get://localhost:7777 && npm run electron:serve-tsc && electron . --serve",
    "electron:serve-tsc": "tsc -p tsconfig-serve.json"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true
  },
  "build": {
    "appId": "com.vmware.tanzu.octant"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.0.0",
    "@angular/common": "^9.0.0",
    "@angular/compiler": "^9.0.0",
    "@angular/core": "^9.0.0",
    "@angular/forms": "^9.0.0",
    "@angular/platform-browser": "^9.0.0",
    "@angular/platform-browser-dynamic": "^9.0.0",
    "@angular/router": "^9.0.0",
    "@clr/angular": "^3.0.0-next.8",
    "@clr/core": "^3.0.0-next.8",
    "@clr/icons": "^3.0.0-next.8",
    "@clr/ui": "^3.0.0-next.8",
    "@ng-select/ng-select": "^3.7.2",
    "@types/cytoscape": "^3.8.6",
    "@webcomponents/custom-elements": "^1.3.1",
    "angular-resizable-element": "^3.2.6",
    "ansi_up": "^4.0.4",
    "core-js": "^2.6.11",
    "cytoscape": "^3.12.1",
    "cytoscape-dagre": "^2.2.2",
    "cytoscape-node-html-label": "^1.1.5",
    "d3-graphviz": "^2.6.1",
    "d3-selection": "^1.4.1",
    "d3-zoom": "^1.8.3",
    "dagre-d3": "^0.6.4",
    "decimal.js": "^10.2.0",
    "default-passive-events": "^1.0.10",
    "highlight.js": "^9.16.2",
    "lodash": "^4.17.15",
    "lodash-es": "^4.17.15",
    "monaco-editor": "^0.20.0",
    "ng-monaco-editor": "^2.2.2",
    "ngx-highlightjs": "^3.0.3",
    "ngx-markdown": "^8.2.1",
    "rxjs": "~6.5.3",
    "serialize-javascript": ">=2.1.1",
    "tslib": "^1.10.0",
    "ws": "^7.2.0",
    "xterm": "^4.3.0",
    "xterm-addon-fit": "^0.3.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.0",
    "@angular/cli": "~9.0.0",
    "@angular/compiler-cli": "~9.0.0",
    "@angular/language-service": "~9.0.0",
    "@types/d3": "~4.11.0",
    "@types/graphlib-dot": "^0.6.1",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "^2.0.8",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.11.1",
    "@types/ws": "^6.0.4",
    "codelyzer": "^5.1.2",
    "dayjs": "^1.8.19",
    "electron": "^8.0.0",
    "electron-builder": "^22.3.2",
    "electron-reload": "^1.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-spec-reporter": "0.0.32",
    "prettier": "^1.19.1",
    "prettier-tslint": "^0.4.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.1.0",
    "typescript": "~3.7.5",
    "wait-on": "^4.0.0"
  }
}
